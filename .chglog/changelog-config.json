{
  "template": "# Changelog\n\n## {{TO_TAG}} ({{TO_TAG_DATE}})\n\n{{CHANGELOG}}",
  "categories": [
    {
      "title": "### Bug Fixes",
      "labels": ["fix", "bug"]
    },
    {
      "title": "### Features",
      "labels": ["feat", "feature"]
    },
    {
      "title": "### Miscellaneous Chores",
      "labels": ["chore", "ci", "deps"]
    }
  ],
  "sort": {
    "order": "DESC",
    "on_property": "mergedAt"
  }
}